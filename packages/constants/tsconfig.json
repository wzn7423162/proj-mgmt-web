{
  "exclude": [
    "build",
    "tests"
  ],
  "extends": "@llama-fa/compile/config/tsconfig.options.json",
  "include": [
    "src/**/*",
    "types/**/*",
    "typings.d.ts",
    "../../types/**/*"
  ],
  "compilerOptions": {
    "outDir": "./build",
    "paths": {
      "@/assets/*": [
        "./src/assets/*"
      ],
      "@/infer/*": [
        "./src/infer/*"
      ],
      "@/llama/*": [
        "./src/llama/*"
      ],
      "@/pay/*": [
        "./src/pay/*"
      ],
      "@/server/*": [
        "./src/server/*"
      ],
      "@/status/*": [
        "./src/status/*"
      ],
      "@/storage/*": [
        "./src/storage/*"
      ],
      "@/styles/*": [
        "./src/styles/*"
      ],
      "@/user/*": [
        "./src/user/*"
      ]
    }
  }
}