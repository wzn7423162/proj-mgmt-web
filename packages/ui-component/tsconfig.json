{
  "exclude": [
    "build",
    "tests"
  ],
  "extends": "@llama-fa/compile/config/tsconfig.options.json",
  "include": [
    "src/**/*",
    "src/**/*.json",
    "types/**/*",
    "typings.d.ts",
    "../../types/**/*",
    "vite.config.mjs"
  ],
  "references": [
    {
      "path": "../utils"
    },
    {
      "path": "../types"
    },
    {
      "path": "../constants/"
    },
    {
      "path": "../core/"
    }
  ],
  "compilerOptions": {
    "outDir": "./build",
    "paths": {
      "@/b-empty-page/*": [
        "./src/b-empty-page/*"
      ],
      "@/b-filter/*": [
        "./src/b-filter/*"
      ],
      "@/b-infinite-list/*": [
        "./src/b-infinite-list/*"
      ],
      "@/upload-model/*": [
        "./src/upload-model/*"
      ]
    }
  }
}