{
  "exclude": [
    "build",
    "tests"
  ],
  "extends": "@llama-fa/compile/config/tsconfig.options.json",
  "include": [
    "src/**/*",
    "types/**/*",
    "typings.d.ts",
    "../../types/**/*"
  ],
  "compilerOptions": {
    "types": [
      "../../types/global.d.ts"
    ],
    "outDir": "./build",
    "paths": {
      "@/chat/*": [
        "./src/chat/*"
      ],
      "@/entity/*": [
        "./src/entity/*"
      ],
      "@/entity-infer/*": [
        "./src/entity-infer/*"
      ],
      "@/infer/*": [
        "./src/infer/*"
      ],
      "@/meta/*": [
        "./src/meta/*"
      ],
      "@/task/*": [
        "./src/task/*"
      ],
      "@/training/*": [
        "./src/training/*"
      ]
    }
  }
}